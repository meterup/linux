#include "qcom-ipq8064.dtsi"

/ {
	aliases {
		serial0 = &uart7;
		serial1 = &gsbi6_serial;
		mdio-gpio0 = &virt_mdio0;
		ethernet0 = &gmac0;
		ethernet1 = &gmac2;
	};

	chosen {
		bootargs-override = "ubi.mtd=rootfs root=/dev/ubiblock0_1 earlyprintk console=ttyMSM0,115200n8";
		stdout-path = "serial0:115200n8";
	};


	memory@0 {
		reg = <0x40000000 0x10000000>;
		device_type = "memory";
	};

	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;
		rsvd@41200000 {
			reg = <0x41200000 0x300000>;
			no-map;
		};
	};

	watchdog: watchdog {
		/* PIC10F202 */
		compatible = "linux,wdt-gpio";
		gpios = <&qcom_pinmux 62 GPIO_ACTIVE_LOW>;
		hw_algo = "toggle";
		hw_margin_ms = <65535>;
	};

	soc {
		pinmux@800000 {
			i2c4_pins: i2c4_pinmux {
				pins = "gpio12", "gpio13";
				function = "gsbi4";
				bias-disable;
			};

			spi_pins: spi_pins {
				mux {
					pins = "gpio18", "gpio19", "gpio21";
					function = "gsbi5";
					drive-strength = <10>;
					bias-none;
				};
				cs {
					pins = "gpio20";
					drive-strength = <12>;
				};
			};
			nand_pins: nand_pins {
				mux {
					pins = "gpio34", "gpio35", "gpio36",
					       "gpio37", "gpio38", "gpio39",
					       "gpio40", "gpio41", "gpio42",
					       "gpio43", "gpio44", "gpio45",
					       "gpio46", "gpio47";
					function = "nand";
					drive-strength = <10>;
					bias-disable;
				};
				pullups {
					pins = "gpio39";
					bias-pull-up;
				};
				hold {
					pins = "gpio40", "gpio41", "gpio42",
					       "gpio43", "gpio44", "gpio45",
					       "gpio46", "gpio47";
					bias-bus-hold;
				};
			};

			mdio0_pins: mdio0_pins {
				mux {
					pins = "gpio0", "gpio1";
					function = "gpio";
					drive-strength = <8>;
					bias-disable;
				};
			};

			rgmii2_pins: rgmii2_pins {
				mux {
					// gpio2 and gpio66 affect the rgmii pin strengths
					// see the note after `Table 3-1 GPIO function configuration`
					// in the IPQ806X Programming Reference Manual.
					// While the WAN port works better in this configuration,
					// it should be noted that gpio66 is connected to QFPROM_EN
					// per the ODM's gpio map.
					pins = "gpio2", "gpio66";
					function = "rgmii2";
					drive-strength = <8>;
					bias-disable;
				};
			};
			gsbi6_uart_2pins: gsbi6_uart_2pins {
				mux {
					pins = "gpio55", "gpio56";
					function = "gsbi6";
					drive-strength = <12>;
					bias-disable;
				};
			};
			leds_pins: leds_pins {
				mux {
					pins = "gpio22", "gpio23", "gpio24",
							"gpio25", "gpio26", "gpio27",
							"gpio30", "gpio51", "gpio65";
					function = "gpio";
					drive-strength = <2>;
					bias-pull-up;
					output-high;
				};
			};
		};

		gsbi5: gsbi@1a200000 {
			qcom,mode = <GSBI_PROT_SPI>;
			status = "ok";

			spi4: spi@1a280000 {
				status = "ok";
				spi-max-frequency = <50000000>;

				pinctrl-0 = <&spi_pins>;
				pinctrl-names = "default";

				cs-gpios = <&qcom_pinmux 20 0>;

				flash: m25p80@0 {
					compatible = "s25fl256s1", "fixed-partitions";
					#address-cells = <1>;
					#size-cells = <1>;
					spi-max-frequency = <50000000>;
					reg = <0>;
				};
			};
		};


               gsbi6: gsbi@16500000 {
			qcom,mode = <GSBI_PROT_I2C_UART>;
			status = "ok";

                        syscon-tcsr = <&tcsr>;

                        gsbi6_serial: serial@16540000 {
                                compatible = "qcom,msm-uartdm-v1.3", "qcom,msm-uartdm";
                                reg = <0x16540000 0x1000>,
                                      <0x16500000 0x1000>;
                                interrupts = <GIC_SPI 156 IRQ_TYPE_LEVEL_HIGH>;
                                clocks = <&gcc GSBI6_UART_CLK>, <&gcc GSBI6_H_CLK>;
                                clock-names = "core", "iface";
				pinctrl-0 = <&gsbi6_uart_2pins>;
				pinctrl-names = "default";
                                status = "ok";
                        };
		};

		gsbi7: gsbi@16600000 {
			qcom,mode = <GSBI_PROT_I2C_UART>;
			status = "ok";
			uart7: serial@16640000 {
				status = "ok";
			};
		};

		sata-phy@1b400000 {
			status = "ok";
		};

		sata@29000000 {
			status = "ok";
		};

		phy@100f8800 {		/* USB3 port 1 HS phy */
			status = "ok";
		};

		phy@100f8830 {		/* USB3 port 1 SS phy */
			status = "ok";
		};

		phy@110f8800 {		/* USB3 port 0 HS phy */
			status = "ok";
		};

		phy@110f8830 {		/* USB3 port 0 SS phy */
			status = "ok";
		};

		usb30@0 {
			status = "ok";
		};

		usb30@1 {
			status = "ok";
		};

		leds {
			compatible = "gpio-leds";
			pinctrl-0 = <&leds_pins>;
			pinctrl-names = "default";

			led@23 {
				label = "mw03:lan";
				gpios = <&qcom_pinmux 23 GPIO_ACTIVE_LOW>;
				default-state = "off";
			};
			led@24 {
				label = "mw03:wlan2g";
				gpios = <&qcom_pinmux 24 GPIO_ACTIVE_LOW>;
				default-state = "off";
			};
			led@25 {
				label = "mw03:wlan5g";
				gpios = <&qcom_pinmux 25 GPIO_ACTIVE_LOW>;
				default-state = "off";
			};
			led@26 {
				label = "mw03:pwr";
				gpios = <&qcom_pinmux 26 GPIO_ACTIVE_LOW>;
				default-state = "on";
			};
			led@27 {
				label = "mw03:wan";
				gpios = <&qcom_pinmux 27 GPIO_ACTIVE_LOW>;
				default-state = "off";
			};
			led@30 {
				label = "BT_LOADER";
				gpios = <&qcom_pinmux 30 GPIO_ACTIVE_HIGH>;
				default-state = "off";
			};
			led@51 {
				label = "QCA8334_RESET";
				gpios = <&qcom_pinmux 51 GPIO_ACTIVE_HIGH>;
				default-state = "off";
			};
			led@65 {
				label = "BT_RESET";
				gpios = <&qcom_pinmux 65 GPIO_ACTIVE_LOW>;
				default-state = "off";
			};
		};

		pcie0: pci@1b500000 {
			status = "ok";
			pcie@0 {
				reg = <0 0 0 0 0>;
				#interrupt-cells = <1>;
				#size-cells = <2>;
				#address-cells = <3>;
				device_type = "pci";

				ath10k@0,0 {
					reg = <0 0 0 0 0>;
					device_type = "pci";
					qcom,mtd-name = "0:ART";
					qcom,cal-offset = <0x1000>;
					qcom,cal-len = <12064>;
				};
			};
		};

		pcie1: pci@1b700000 {
			status = "ok";
			force_gen1 = <1>;
				ath10k@0,0 {
					reg = <0 0 0 0 0>;
					device_type = "pci";
					qcom,mtd-name = "0:ART";
					qcom,cal-offset = <0x5000>;
					qcom,cal-len = <12064>;
				};
		};

		pcie2: pci@1b900000 {
			status = "ok";
				ath10k@0,0 {
					reg = <0 0 0 0 0>;
					device_type = "pci";
					qcom,mtd-name = "0:ART";
					qcom,cal-offset = <0x9000>;
					qcom,cal-len = <2116>;
				};
		};

		nand-controller@1ac00000 {
			status = "ok";

			pinctrl-0 = <&nand_pins>;
			pinctrl-names = "default";

			cs0 {
				reg = <0>;
				compatible = "qcom,nandcs";

				nand-ecc-strength = <4>;
				nand-bus-width = <8>;
				nand-ecc-step-size = <512>;

				partitions {
					compatible = "qcom,qcom_nand", "fixed-partitions";
					#address-cells = <1>;
					#size-cells = <1>;
				};
			};
		};

		virt_mdio0: mdio {
			compatible = "virtual,mdio-gpio";
			#address-cells = <1>;
			#size-cells = <0>;
			gpios = <&qcom_pinmux 1 0 &qcom_pinmux 0 0>;
			pinctrl-0 = <&mdio0_pins>;
			pinctrl-names = "default";

			phy0: ethernet-phy@0 {
				reg = <0>;
				qca,ar8327-initvals = <
					0x00004 0x7600000   /* PAD0_MODE */
					0x00008 0x1000000   /* PAD5_MODE */
					0x0000c 0x20080     /* PAD6_MODE */
					0x000e4 0x6a545     /* MAC_POWER_SEL */
					0x000e0 0xc74164de  /* SGMII_CTRL */
					0x0007c 0x4e        /* PORT0_STATUS */
					0x00094 0x4e        /* PORT6_STATUS */
					>;
			};

			ethernet-phy@2 {
				device_type = "ethernet-phy";
				reg = <2>;
				qca,phy-rgmii-en;
				qca,txclk-delay-en;
				qca,rxclk-delay-en;
			};

			ethernet-phy@1 {
				device_type = "ethernet-phy";
				reg = <1>;
			};
		};

		gmac0: ethernet@37000000 {
			status = "ok";
			phy-mode = "rgmii";
			qcom,id = <0>;

			pinctrl-0 = <&rgmii2_pins>;
			pinctrl-names = "default";
			mdiobus = <&virt_mdio0>;

			fixed-link {
				speed = <1000>;
				full-duplex;
			};
		};

		gmac2: ethernet@37400000 {
			status = "ok";
			phy-mode = "sgmii";
			qcom,id = <2>;
			mdiobus = <&virt_mdio0>;

			fixed-link {
				speed = <1000>;
				full-duplex;
			};
		};

		timer@200a000 {
			status = "okay";
		};
	};
};

&gsbi4 {
    status = "disabled";
};

&adm_dma {
	status = "ok";
};
